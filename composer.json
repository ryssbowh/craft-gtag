{
    "name": "ryssbowh/craft-gtag",
    "description": "Google tag integration",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "gtag"
    ],
    "support": {
        "docs": "https://github.com/ryssbowh/craft-gtag/blob/master/README.md",
        "issues": "https://github.com/ryssbowh/craft-gtag/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "The Web Puzzlers",
            "homepage": "https://github.com/ryssbowh"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0-beta.1",
        "php": "7.x | 8.x"
    },
    "autoload": {
        "psr-4": {
          "Ryssbowh\\Gtag\\": "src/"
        }
    },
    "extra": {
        "name": "Google Tags",
        "handle": "gtag",
        "documentationUrl": "https://github.com/ryssbowh/craft-gtag",
        "class": "Ryssbowh\\Gtag\\Gtag"
    }
}
